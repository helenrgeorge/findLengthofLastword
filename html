/**
 * @param {string} s
 * @return {number}
 */


var lengthOfLastWord = function(s) {
    let position = s.length - 1
        while (s[position] == " ") {
            position = position - 1
        }
    let wordSize = 0
    while (s[position] != " ") {
        position = position - 1 
        wordSize = wordSize + 1
    }
    return wordSize
    
    
    
};
